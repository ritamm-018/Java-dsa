import java.util.*;

public class Main{
    public static int indexOfMaxElement(int[] arr){
        int min = arr[0];
        int minIndex = 0;

        for(int i = 0; i < arr.length; i ++){
            if(arr[i] < min){
                min = arr[i];
                minIndex = i;
            }
        }
        return minIndex;
    }
    public static void main(String[] args){
        int[] arr = {7, 3, 5, 1, 9};
        System.out.println(indexOfMaxElement(arr));
    }
}
