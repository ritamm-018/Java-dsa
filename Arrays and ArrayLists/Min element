import java.util.*;

public class Main{
    public static int findMin(int[] arr){
        int min = arr[0];
        for(int i = 0; i < arr.length; i++){
            if(arr[i] < min){
                min = arr[i];
            }
        }
        return min;
    }
    public static void main(String[] args){
        int[] arr = {8, 4, 1, 9, 2};
        System.out.println(findMin(arr));
    }
}
